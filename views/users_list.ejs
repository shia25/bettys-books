<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User List</title>
</head>
<body>
    <h1>Registered Users</h1>
    <ul>
        <% userList.forEach(function(user) { %>
            <li>
                Username: <%= user.username %><br>
                Name: <%= user.first_name %> <%= user.last_name %><br>
                Email: <%= user.email %><br>
            </li>
        <% }) %>
    </ul>
</body>
</html>
